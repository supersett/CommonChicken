<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chicken.mapper.MemberMapper">
	<insert id="insertMember">
		insert into member_tb values(#{mem_email}, #{mem_nickname}, #{mem_pw}, #{mem_name}, #{mem_phone}, 
										#{mem_zip_code}, #{mem_add1}, #{mem_add2}, #{mem_photo}, #{mem_join_date}, 
										#{mem_status}, #{mem_last_login}, #{mem_birthday}, #{mem_auth_code})
	</insert>
	
	<select id="selectMemberList" resultType="MemberDTO">
		select * from member_tb order by mem_email
	</select>
</mapper>